{
  "entryPoints": ["src/components/index.ts"],
  "out": "docs",
  "plugin": ["typedoc-plugin-markdown"],
  "excludeExternals": true,
  "excludePrivate": true,
  "excludeProtected": true,
  "exclude": ["**/node_modules/**"],
  "excludeNotDocumented": true,
  "excludeNotDocumentedKinds": ["Interface", "TypeAlias","Module",
        "Namespace",
        "Class",
        "Constructor",
        "CallSignature",
        "IndexSignature",
        "ConstructorSignature",
        "Accessor",
        "GetSignature",
        "SetSignature",
        "TypeAlias",
        "Reference"],
  "hideGenerator": true,
  "name": "blueshift-lit-components",
  "includeVersion": true
}
